{
  "name": "socendani/skeleton-slim3-apirest",
  "description": "A Slim Framework skeleton for me ;-)",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/slimphp/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Dani Morte",
      "email": "socendani@gmail.com",
      "homepage": "https://www.lacasalila.com/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/database": "~5.4",
    "respect/validation": "^1.1",
    "slim/slim": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Src\\": "Src/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public"
  }
}
